<Context path="/choices"
<% if @specify_docbase == "true" -%>
docBase="<%= @node[:hg_mde][:hg_war_path] %>/<%= @node[:hg_mde][:hg_version] %>.war"
<% end -%>
	    reloadable="false"
	    debug="0">

    <Manager className="org.apache.catalina.session.PersistentManager" saveOnRestart="false"/>
    
    <Parameter name="feedbackEmailTo" override="false" value="healthguides-feedback@mapofmedicine.com"/>
    <Parameter name="feedbackEmailFrom" override="false" value="support@mapofmedicine.com"/>
    <Parameter name="feedbackEmailSMTP" override="false" value="127.0.0.1"/>

</Context>
